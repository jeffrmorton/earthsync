      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" }, "editorMode": "code",
          "expr": "sum(rate(peaks_detected_total{job=\"earthsync-server\"}[5m])) by (detectorId)",
          "legendFormat": "{{detectorId}} Peaks/sec", "range": true, "refId": "A"
